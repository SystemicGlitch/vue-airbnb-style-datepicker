{
  "name": "vue-airbnb-style-datepicker",
  "version": "2.7.0",
  "description": "A VueJs version of the popular AirBnb datepicker",
  "type": "module",
  "main": "dist/vue-airbnb-style-datepicker.cjs.js",
  "module": "dist/vue-airbnb-style-datepicker.es.js",
  "unpkg": "dist/vue-airbnb-style-datepicker.js",
  "repository": {
    "url": "MikaelEdebro/vue-airbnb-style-datepicker",
    "type": "git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env SASS_SILENCE_DEPRECATIONS=all vite",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "cross-env SASS_SILENCE_DEPRECATIONS=all vite build -c vite.lib.config.ts",
    "build:demo": "cross-env SASS_SILENCE_DEPRECATIONS=all vite build",
    "preview": "cross-env SASS_SILENCE_DEPRECATIONS=all vite preview --port 5052",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:debug": "vitest --inspect",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "npm run build:lib"
  },
  "author": "MikaelEdebro <mikael.edebro@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "date-fns": "^1.29.0",
    "jsdom": "^24.0.0",
    "vue": "^3.4.0",
    "prettier": "^3.2.5",
    "rimraf": "^2.6.2",
    "sass-embedded": "^1.70.0",
    "vite": "^5.0.0",
    "vitest": "^1.4.0"
  },
  "peerDependencies": {
    "date-fns": "1.x",
    "vue": "3.x"
  },

  "dependencies": {
    "v-click-outside": "^3.0.0"
  }
}
